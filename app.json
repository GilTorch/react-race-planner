{
  "expo": {
    "name": "ScriptoRerum",
    "slug": "scriptorerum",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "experiments": {
      "redesignedLogBox": true
    },
    "version": "1.0.7",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sr",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "facebookScheme": "fb2868459866594129",
    "facebookAppId": "2868459866594129",
    "facebookDisplayName": "ScriptoRerum",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.scriptorerum.ScriptoRerum",
      "googleServicesFile": "./google-services.json"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.scriptorerum.ScriptoRerum"
    },
    "description": "The demo of the ScriptoRerum mobile app for iOS and Android.",
    "githubUrl": "https://github.com/NouKod-Haiti/sr-mobile",
    "sdkVersion": "38.0.0",
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}
